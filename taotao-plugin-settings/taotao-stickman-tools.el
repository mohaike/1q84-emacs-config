(fset 'stickman-ui-change
      (lambda (&optional arg)
        "Keyboard macro. run eshell command"
        (interactive "p")
        (kmacro-exec-ring-item
         (quote
          ([?\M->
            ?.
            ? 
            ?/ ?U ?s ?e ?r ?s ?/
            ?1 ?q ?8 ?4 ?/
            ?D ?o ?c ?u ?m ?e ?n ?t ?s ?/
            ?t ?e ?m ?p ?/
            ?S ?t ?i ?c ?k ?M ?a ?n ?/
            ?t ?o ?o ?l ?s ?/
            ?U ?I ?R ?e ?f ?l ?a ?s ?h ?T ?o ?o ?l ?s ?/
            ?u ?i ?C ?h ?a ?n ?g ?e ?. ?s ?h
            return]
           0 "%d"))
         arg)))

